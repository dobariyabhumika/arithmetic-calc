{
    "name": "calc/arithmetic",
    "description": "package for calculate arithmetic functions",
    "keywords": [
        "greatest common divisor (gcd)",
        "greatest common divisor for more then three number",
        "least common multiple (lcm)",
        "least common multiple for more then three number",
        "average of numbers",
        "factors of numbers",
        "primeFactors of numbers",
        "weighted average of multiple numbers with different weight"
    ],
    "type": "library",
    "version": "1.0.2",
    "authors": [
        {
            "name": "bhumika",
            "email": "bhumika@improwised.com"
        }
    ],
    "minimum-stability": "dev-master",
    "require": {
        "php": "^5.3.3 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.2-dev"
        }
    },
    "autoload": {
      "psr-4": {
        "Calc\\Arithmetic\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
