{
    "name": "calc/arithmetic",
    "description": "package for calculate arithmetic functions: GCD, LCM, average, weighted average, factors and primeFactors",
    "keywords": [
        "gcd",
        "lcm",
        "greatestCommonDivisor",
        "greatestCommonDivisorForMultipleNumbers",
        "leastCommonMultiple",
        "leastCommonMultipleForMultipleNumbers",
        "average",
        "factors",
        "primeFactors",
        "weightedAverage"
    ],
    "type": "library",
    "version": "1.0.2",
    "authors": [
        {
            "name": "bhumika",
            "email": "bhumika@improwised.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.3.3 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.2"
        }
    },
    "autoload": {
      "psr-4": {
        "Calc\\Arithmetic\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
